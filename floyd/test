#! /data/data/com.termux/files/usr/bin/bash

network_name=$(python -c "import name; print(name.network_name)")

floyd init cifar10/$network_name/test

data_output_id=$(cat floyd/ids.yml | shyaml get-value data.output)
model_output_id=$(cat floyd/ids.yml | shyaml get-value train.output)

floyd run \
  --gpu \
  --env tensorflow:py2 \
  --data $data_output_id:data \
  --data $model_output_id:model \
  "DATAGET_HOME=/data MODEL_PATH=/model python test.py"
